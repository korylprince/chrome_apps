{
    "manifest_version": 2,
    "name": "iRead Autofill",
    "description": "Autofills iRead credentials",
    "version": "1.5",
    "update_url": "https://korylprince.github.io/chrome_apps/iread_autofill/update.xml",
    "permissions": ["*://idp-awsprod1.education.scholastic.com/idp/*"],
    "icons": {
        "16": "icon16.png",
        "24": "icon24.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [{
        "matches": ["*://idp-awsprod1.education.scholastic.com/idp/*"],
        "js": ["autofill.js"]
    }]
}
